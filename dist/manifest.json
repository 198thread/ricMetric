{
    "manifest_version": 3,
    "name": "staxon",
    "version": "1.0",
    "permissions": ["scripting", "activeTab"],
    "background": {
        "scripts": [
          "background.js"
        ]
      },
      "content_scripts": [
        {
          "matches": [ "<all_urls>" ],
          "js": [
            "content.js"
          ]
        }
    ]
}